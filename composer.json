{
  "name": "floor12/yii2-superfile",
  "description": "Yii2 extention to upload files (images, video, files) to your models",
  "type": "yii2-extension",
  "minimum-stability": "stable",
  "keywords": [
    "floor12",
    "yii",
    "yii2",
    "yii 2"
  ],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "floor12",
      "email": "primelifecoder@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "yiisoft/yii2": "~2.0.9",
    "yiisoft/yii2-jui": "*",
    "bower-asset/cropper": "*",
    "bower-asset/ng-file-upload": "*",
    "tesjin/yii2-angularjs": ">=1.3.8"
  },
  "autoload": {
    "psr-4": {
      "floor12\\superfile\\": [
        "src",
        "src/assets",
        "src/logic",
        "src/controllers"
      ]
    }
  }
}
